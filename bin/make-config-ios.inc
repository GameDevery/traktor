# Configuration file

# Toolchain and SDK configuration.
XCODE_PATH=$(shell xcode-select --print-path)
TOOLCHAIN_BIN=$(XCODE_PATH)/Toolchains/XcodeDefault.xctoolchain/usr/bin
SDK_SYSROOT=$(XCODE_PATH)/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS9.1.sdk
ARCH=armv7
IPHONE_OS_MIN_VERSION=7.1

# Build tools.
CC=$(TOOLCHAIN_BIN)/clang
CC_FLAGS_COMMON=-arch $(ARCH) -fmessage-length=0 -fdiagnostics-show-note-include-stack -fmacro-backtrace-limit=0 -fstrict-aliasing -fvisibility=hidden -fpascal-strings -isysroot $(SDK_SYSROOT) -miphoneos-version-min=$(IPHONE_OS_MIN_VERSION)
CC_FLAGS_WARNINGS=
CC_FLAGS_DEBUG_SHARED=$(CC_FLAGS_COMMON) $(CC_FLAGS_WARNINGS) -g -O0 -D _UNICODE -D UNICODE -D __IOS__ -DDEBUG=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0 
CC_FLAGS_RELEASE_SHARED=$(CC_FLAGS_COMMON) $(CC_FLAGS_WARNINGS) -g -O3 -D _UNICODE -D UNICODE -D __IOS__ -DNS_BLOCK_ASSERTIONS=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0 
CC_FLAGS_DEBUG_STATIC=$(CC_FLAGS_COMMON) $(CC_FLAGS_WARNINGS) -g -O0 -D _UNICODE -D UNICODE -D __IOS__ -DDEBUG=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
CC_FLAGS_RELEASE_STATIC=$(CC_FLAGS_COMMON) $(CC_FLAGS_WARNINGS) -g -O3 -D _UNICODE -D UNICODE -D __IOS__ -DNS_BLOCK_ASSERTIONS=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
CC_FLAGS_DEBUG_EXECUTABLE=$(CC_FLAGS_COMMON) $(CC_FLAGS_WARNINGS) -g -O0 -D _UNICODE -D UNICODE -D __IOS__ -DDEBUG=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
CC_FLAGS_RELEASE_EXECUTABLE=$(CC_FLAGS_COMMON) $(CC_FLAGS_WARNINGS) -g -O3 -D _UNICODE -D UNICODE -D __IOS__ -DNS_BLOCK_ASSERTIONS=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0 
CC_FLAG_OUTPUT=-o 

MM=$(TOOLCHAIN_BIN)/clang -x objective-c++
MM_FLAGS_COMMON=-arch $(ARCH) -fmessage-length=0 -fdiagnostics-show-note-include-stack -fmacro-backtrace-limit=0 -fstrict-aliasing -fvisibility=hidden -fpascal-strings -isysroot $(SDK_SYSROOT) -miphoneos-version-min=$(IPHONE_OS_MIN_VERSION)
MM_FLAGS_WARNINGS=
MM_FLAGS_DEBUG_SHARED=$(MM_FLAGS_COMMON) $(MM_FLAGS_WARNINGS) -g -O0 -D _UNICODE -D UNICODE -D __IOS__ -DDEBUG=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
MM_FLAGS_RELEASE_SHARED=$(MM_FLAGS_COMMON) $(MM_FLAGS_WARNINGS) -g -O3 -D _UNICODE -D UNICODE -D __IOS__ -DNS_BLOCK_ASSERTIONS=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
MM_FLAGS_DEBUG_STATIC=$(MM_FLAGS_COMMON) $(MM_FLAGS_WARNINGS) -g -O0 -D _UNICODE -D UNICODE -D __IOS__ -DDEBUG=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
MM_FLAGS_RELEASE_STATIC=$(MM_FLAGS_COMMON) $(MM_FLAGS_WARNINGS) -g -O3 -D _UNICODE -D UNICODE -D __IOS__ -DNS_BLOCK_ASSERTIONS=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
MM_FLAGS_DEBUG_EXECUTABLE=$(MM_FLAGS_COMMON) $(MM_FLAGS_WARNINGS) -g -O0 -D _UNICODE -D UNICODE -D __IOS__ -DDEBUG=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
MM_FLAGS_RELEASE_EXECUTABLE=$(MM_FLAGS_COMMON) $(MM_FLAGS_WARNINGS) -g -O3 -D _UNICODE -D UNICODE -D __IOS__ -DNS_BLOCK_ASSERTIONS=1 -DOBJC_OLD_DISPATCH_PROTOTYPES=0
MM_FLAG_OUTPUT=-o 

AR=$(TOOLCHAIN_BIN)/libtool
AR_FLAGS_COMMON=-static -syslibroot $(SDK_SYSROOT)
AR_FLAGS_DEBUG_STATIC=$(AR_FLAGS_COMMON)
AR_FLAGS_RELEASE_STATIC=$(AR_FLAGS_COMMON)
AR_FLAG_OUTPUT=-o 

LINK=$(TOOLCHAIN_BIN)/clang $(CC_FLAGS_COMMON)
LINK_FLAG_OUTPUT=-o 
LINK_LIBS=

BINARY_INCLUDE=BinaryInclude
