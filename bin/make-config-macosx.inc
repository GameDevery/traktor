# Configuration file

CC=gcc

CC_FLAGS_COMMON=-arch i386 -fno-strict-aliasing -fno-common

CC_FLAGS_DEBUG_SHARED=$(CC_FLAGS_COMMON) -g -D _UNICODE -D UNICODE -fPIC
CC_FLAGS_RELEASE_SHARED=$(CC_FLAGS_COMMON) -O3 -ffast-math -D _UNICODE -D UNICODE -fPIC

CC_FLAGS_DEBUG_STATIC=$(CC_FLAGS_COMMON) -g -D _UNICODE -D UNICODE
CC_FLAGS_RELEASE_STATIC=$(CC_FLAGS_COMMON) -O3 -ffast-math -D _UNICODE -D UNICODE

CC_FLAGS_DEBUG_EXECUTABLE=$(CC_FLAGS_COMMON) -g -D _UNICODE -D UNICODE
CC_FLAGS_RELEASE_EXECUTABLE=$(CC_FLAGS_COMMON) -O3 -ffast-math -D _UNICODE -D UNICODE

MM=gcc

MM_FLAGS_COMMON=-fno-strict-aliasing -fno-common -Wall -Wundef -Wno-ctor-dtor-privacy

MM_FLAGS_DEBUG_SHARED=$(MM_FLAGS_COMMON) -g -D _UNICODE -D UNICODE -fPIC
MM_FLAGS_RELEASE_SHARED=$(MM_FLAGS_COMMON) -D _UNICODE -D UNICODE -fPIC

MM_FLAGS_DEBUG_STATIC=$(MM_FLAGS_COMMON) -g -D _UNICODE -D UNICODE
MM_FLAGS_RELEASE_STATIC=$(MM_FLAGS_COMMON) -D _UNICODE -D UNICODE

MM_FLAGS_DEBUG_EXECUTABLE=$(MM_FLAGS_COMMON) -g -D _UNICODE -D UNICODE
MM_FLAGS_RELEASE_EXECUTABLE=$(MM_FLAGS_COMMON) -D _UNICODE -D UNICODE

AR=libtool

LINK=c++
LINK_LIBS=

ADDITIONAL_INCLUDE_PATHS=
ADDITIONAL_LIBRARY_PATHS=

BINARY_INCLUDE=$(TRAKTOR_HOME)/bin/MacOSX/BinaryInclude
