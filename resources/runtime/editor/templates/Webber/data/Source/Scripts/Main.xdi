Main = Main or class("Main")

function Main:run()
	local httpServer = HttpServer()
	httpServer:create(1337)
	httpServer:setRequestListener(HttpRequestListener(
		function()
			return MyApplication(environment:getDatabase())
		end,
		false
	))
	while true do
		httpServer:update(100)
	end
end

