			"request": "launch",

			"type": "cppdbg",
			"program": "${workspaceRoot}/../bin/latest/linux/debugshared/Traktor.Editor.App",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}/..",
