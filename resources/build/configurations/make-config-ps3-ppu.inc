# C

C="$(SCE_PS3_ROOT)\host-win32\sn\bin\ps3ppusnc.exe"

C_SYSTEM_DEFINES=-D__PS3__ -D_PS3 -DSN_TARGET_PS3 -D__SNC__ -D__CELLOS_LV2__ -DUNICODE -D_UNICODE
C_SYSTEM_INCLUDES=-I$(SCE_PS3_ROOT)\target\ppu\include -I$(SCE_PS3_ROOT)\target\common\include -I$(SCE_PS3_ROOT)\host-win32\sn\ppu\include

