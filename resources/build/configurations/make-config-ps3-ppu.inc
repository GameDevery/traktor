C_FLAGS_RELEASE_EXECUTABLE=$(C_FLAGS) $(C_FLAGS_RELEASE)
C_FLAGS_RELEASE_EXECUTABLE_CONSOLE=$(C_FLAGS) $(C_FLAGS_RELEASE)

C_FLAG_OUTPUT=-o


