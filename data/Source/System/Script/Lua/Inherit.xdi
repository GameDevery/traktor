<?xml version="1.0" encoding="utf-8"?>
<object type="traktor.script.Script" version="1">
	<dependencies/>
	<text>
	<![CDATA[
-- Setup simple inheritance
function inherit(baseClass)
	local newClass = {}
	setmetatable(newClass, { __index = baseClass })
	return newClass
end

-- Check if instance is of a class type.
function isa(instance, class)
	return instance ~= nil and getmetatable(instance).__index == class
end


	]]>
	</text>
</object>
