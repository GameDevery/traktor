		<item type="traktor.render.Script">
			<comment/>
			<position>
				<first>1018</first>
				<second>-527</second>
			</position>
			<name>DistanceToLine</name>
			<inputPins>
				<item>
					<name>LineStart</name>
					<type>PtVector</type>
					<samplerId/>
				</item>
				<item>
					<name>LineEnd</name>
					<type>PtVector</type>
					<samplerId/>
				</item>
				<item>
					<name>Position</name>
					<type>PtVector</type>
					<samplerId/>
				</item>
			</inputPins>
			<outputPins>
				<item>
					<name>Distance</name>
					<type>PtScalar</type>
				</item>
			</outputPins>
			<samplers/>
			<script>
			<![CDATA[
ENTRY
{
	float2 d = LineEnd.xy - LineStart.xy;
	float dln = length(d);

	float2 dp = float2(d.y, -d.x);
	float2 dpn = dp / dln;

	float k1 = dot(dpn, Position.xy - LineStart.xy);

	float2 Pl = Position.xy - dpn * k1;
	float k2 = dot(Pl - LineStart.xy, d) / (dln * dln);

	if (k2 >= 0.0f && k2 <= 1.0f)
		Distance = abs(k1);
	else
		Distance = min(length(Position.xy - LineStart.xy), length(Position.xy - LineEnd.xy));
}
			]]>
			</script>
