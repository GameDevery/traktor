-- MouseWheelEvent

MouseWheelEvent = MouseWheelEvent or { mask = 0x00000200 }
MouseWheelEvent_meta = MouseWheelEvent_meta or { name = "MouseWheelEvent", __index = MouseWheelEvent }

function MouseWheelEvent.new(x, y, delta)
	local o = {}
	setmetatable(o, MouseWheelEvent_meta)

	o.x = x
	o.y = y
	o.delta = delta

	return o
end

