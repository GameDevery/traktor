-- MouseMoveEvent

MouseMoveEvent = MouseMoveEvent or { mask = 0x00000020 }
MouseMoveEvent_meta = MouseMoveEvent_meta or { name = "MouseMoveEvent", __index = MouseMoveEvent }

function MouseMoveEvent.new(x, y)
	local o = {}
	setmetatable(o, MouseMoveEvent_meta)

	o.x = x
	o.y = y

	return o
end
