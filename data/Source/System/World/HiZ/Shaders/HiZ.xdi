				<item>
					<id>{98C61F79-5BF1-9D49-92B7-77AEEA0EDFAF}</id>
					<name>WorkSize</name>
				</item>
			</inputPins>
			<outputPins/>
			<script>
			<![CDATA[
ivec2 sz = imageSize($Output);
ivec2 pt = ivec2(gl_GlobalInvocationID.xy);

if (pt.x < sz.x && pt.y < sz.y)
{
//	if (pt.x < $WorkSize.x && pt.y < $WorkSize.y)
	{
		ivec2 o = ivec2(1, 0);
		ivec2 p = pt * 2;
	
		float z1 = imageLoad($Input, p + o.yy).x;
		float z2 = imageLoad($Input, p + o.xy).x;
		float z3 = imageLoad($Input, p + o.yx).x;
		float z4 = imageLoad($Input, p + o.xx).x;
	
		float mxz = max(max(z1, z2), max(z3, z4));
		imageStore($Output, pt, vec4(mxz));
	}
}

			]]>
			</script>
		</item>
