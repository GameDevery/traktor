			<comment/>
			<position>
				<first>-402</first>
				<second>472</second>
			</position>
			<name>Decode_GLSL</name>
			<inputPins>
				<item>
					<id>{DA9ACBD6-F7FA-A94E-8899-6FA792C62441}</id>
					<name>Input</name>
				</item>
			</inputPins>
			<outputPins>
				<item>
					<id>{5831C208-971E-4141-AF3D-ABEAEEF2124A}</id>
					<name>Output</name>
					<type>PtVector</type>
				</item>
			</outputPins>
			<script>
			<![CDATA[
ENTRY
{
	vec2 f = Input.xy * 2.0 - 1.0;
	vec3 n = vec3(f.x, f.y, 1.0 - abs(f.x) - abs(f.y));
	float t = clamp(-n.z, 0.0, 1.0);
	vec2 tt = vec2(t, t);
	n.xy += mix(tt, -tt, greaterThanEqual(n.xy, vec2(0.0, 0.0)));
	Output = vec4(normalize(n), 0.0);
}
			]]>
			</script>
		</item>
