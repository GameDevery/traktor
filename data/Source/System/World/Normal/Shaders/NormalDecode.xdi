			<outputPins>
				<item>
					<id>{5831C208-971E-4141-AF3D-ABEAEEF2124A}</id>
					<name>Output</name>
					<type>PtVector</type>
				</item>
			</outputPins>
			<script>
			<![CDATA[
ENTRY
{
	float2 f = Input.xy * 2.0f - 1.0f;
	float3 n = float3(f.x, f.y, 1.0f - abs(f.x) - abs(f.y));
	float t = saturate(-n.z);
	float2 tt = float2(t, t);
	n.xy += (n.xy >= float2(0.0f, 0.0f)) ? tt : -tt;
	Output = float4(normalize(n), 0.0f);
}
			]]>
			</script>
