		<item type="traktor.render.Script" version="5,traktor.render.Node:1">
			<id>{5A11D6CE-EC0C-4447-A1B6-B6F076063D79}</id>
			<comment/>
			<position>
				<first>49</first>
				<second>513</second>
			</position>
			<name>Trace</name>
			<technique/>
			<include/>
			<inputPins>
				<item>
					<id>{8E56F1F6-7778-46DF-B30D-4D4180A7D754}</id>
					<name>ViewPosition</name>
				</item>
				<item>
					<id>{BA1BCA9A-E79A-4217-9416-852306133BFC}</id>
					<name>LightDirection</name>
				</item>
				<item>
					<id>{5463B71C-B2B4-4CEF-9936-0503E476D726}</id>
					<name>Projection</name>
				</item>
				<item>
					<id>{C22C982C-CA5D-4877-A119-2F11D98A2490}</id>
					<name>DepthMap</name>
				</item>
				<item>
					<id>{B3E4E99F-0C8C-4B33-BE92-4F78706A2A61}</id>
					<name>DepthMapState</name>
				</item>
			</inputPins>
			<outputPins>
				<item>
					<id>{77737E13-A2EE-4B2B-BAE1-1D6299C467B1}</id>
					<name>Hit</name>
					<type>Scalar</type>
				</item>
			</outputPins>
			<script>
			<![CDATA[
const float traceLength = 0.2f;
const int traceSteps = 16;

$Hit = 0.0f;

vec3 pos = $ViewPosition.xyz;
if (pos.z < 4.0f)
{
	vec3 stp = $LightDirection.xyz * traceLength / traceSteps;
	for (int i = 0; i < traceSteps; ++i)
	{
		pos += stp;
	
		// Read depth at step position.
		vec4 t = $Projection * vec4(pos, 1.0f);
		t.xy /= t.w;
		t.xy *= vec2(0.5f, -0.5f);
		t.xy += vec2(0.5f, 0.5f);
	
		if (t.x < 0.0f || t.x > 1.0f || t.y < 0.0f || t.y > 1.0f)
			break;
	
		const float depth = texture(sampler2D($DepthMap, $DepthMapState), t.xy).x;
		const float e = pos.z - depth;
	
		if (e > 0.02f && e < 0.1f)
		{
			$Hit = 1.0f;
			break;
		}
	
	}
}


			]]>
			</script>
		</item>
