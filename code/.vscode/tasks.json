{
    "version": "2.0.0",
	"tasks": [
		{
			"label": "DebugShared",
			"type": "shell",
			"command": "${workspaceRoot}/../scripts/build-vscode.sh DebugShared",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "ReleaseShared",
			"type": "shell",
			"command": "${workspaceRoot}/../scripts/build-vscode.sh ReleaseShared",
			"group": "build",
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}