function update(info)
	local cubeEntity = world:getEntity("Cube")
	cubeEntity:setTransform(Transform(
		Vector4(0, 0, 0, 1),
		Quaternion.fromEulerAngles(info:getSimulationTime(), 0, 0)
	))
end

