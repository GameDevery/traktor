<?xml version="1.0" encoding="utf-8"?>
<object type="traktor.script.Script" version="2">
	<text>
	<![CDATA[
-- CubeScript
import(traktor)

CubeScript = CubeScript or inherit(traktor.world.ComponentEntity)
CubeScript_meta = CubeScript_meta or { __index = CubeScript }

function CubeScript.new(o)
	o = o or {}
	setmetatable(o, CubeScript_meta)

	o.angle = 0

	return o
end

function CubeScript:update(totalTime, deltaTime)
	self.angle = self.angle + deltaTime
	self:setTransform(Transform(
		Vector4.zero,
		Quaternion.fromEulerAngles(self.angle, self.angle * 0.66, self.angle * 0.33)
	))
end

function CubeScript:reset()
	self.angle = 0
end


	]]>
	</text>
</object>
